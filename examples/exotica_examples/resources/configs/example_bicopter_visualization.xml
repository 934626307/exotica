    <Scene>
      <JointGroup>floating_base</JointGroup>
      <URDF>{exotica_examples}/resources/robots/bicopter.urdf</URDF>
      <SRDF>{exotica_examples}/resources/robots/bicopter.srdf</SRDF>
      <SetRobotDescriptionRosParams>1</SetRobotDescriptionRosParams>
