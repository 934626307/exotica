  <Maps>
    <EffPosition Name="Position">
      <EndEffector>
          <Frame Link="base_link" />
      </EndEffector>
    </EffPosition>
  </Maps>

  <Cost>
    <Task Task="Position"/>
  </Cost>
</BicopterDynamicProblem>

</ExampleConfig>
